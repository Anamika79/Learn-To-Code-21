#include <stdio.h>

int main() {
    int size,i,key,a[50],get=0;
    scanf("%d\n",&size);
    for(i=0;i<size;i++){
        scanf("%d\n",&a[i]);
    }
    scanf("%d",&key);
    for(i=0;i<size;i++){
        if(a[i]==key){
            get = 1;
            break;
        }
    }
    if(get == 1){
        printf("%d is present in this array",key);
    }
    else{
        printf("%d is not present in this array",key);
    }
    
}
