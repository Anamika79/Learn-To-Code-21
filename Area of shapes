#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    int s,r,b,h;
    int A1;
    float A2,A3;
    scanf("%d",&s);
    scanf("%d",&r);
    scanf("%d",&b);
    scanf("%d",&h);
    A1 = s*s;
    A2 = 3.14*r*r;
    A3 = 0.5*b*h;
    printf("%d\n%.1f\n%.2f\n",A1,A2,A3);
    
}
