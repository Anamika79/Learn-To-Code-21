#include <stdio.h>

int main() {
    int n,a[100],i;
    int *ptr = a;
    scanf("%d",&n);
    for(i=0;i<n;i++){
        scanf("%d",&a[i]);
    }
    printf("DISPLAYING USING INDEX: ");
    for(i=0;i<n;i++){
        printf("%d ",a[i]);
    } 
    printf("\nDISPLAYING USING POINTERS: ");
    for(i=0;i<n;i++){
        printf("%d ",*ptr);
        ptr++;
    }

}
