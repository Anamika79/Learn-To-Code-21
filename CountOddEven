#include <stdio.h>

int main() {
    int size,a[50],i,counteven=0,countodd=0;
    scanf("%d\n",&size);
    for(i=0;i<size;i++){
        scanf("%d\n",&a[i]);
    }
    for(i=0;i<size;i++){
        if(a[i]%2==0){
            counteven = counteven+1;
        }
        else{
            countodd = countodd+1;
        }
    }
    printf("%d\n",counteven);
    printf("%d",countodd);
}
