#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    int n,a[10],i;
    scanf("%d",&n);
    for(i=0;i<n;i++){
        scanf("\n%d",&a[i]);
    }
    printf("Entered array:");
    for(i=0;i<n;i++){
        printf(" %d",a[i]);
    }
    printf("\n%d",a[1]);
}
