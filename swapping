#include <stdio.h>

    void swap(int number1,int number2){
        int temp = number1;
        number1 = number2;
        number2 = temp;
        printf("a=%d b=%d",number1,number2);
    }
    int main() {
        int num1,num2;
        scanf("%d %d",&num1,&num2);
        printf("a=%d b=%d\n",num1,num2);
        swap(num1,num2);
        return 0;
}
